
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT40'
	option country 'US'
	option txpower '20'
	option channel '3'

# Hardcoded wifi keys are temporary!  Will not be used in final version.

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option encryption 'psk2'
	option key 'cloakaccess'
	option ssid 'open'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'cloaked'
	option network 'cloaked'
	option encryption 'psk2+ccmp'
	option key 'cloakaccess'

